{
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Args": [
        "--max-line-length",
        "88"
    ],
    "python.formatting.provider": "black",
    "python.linting.mypyEnabled": true,
    "files.exclude": {
        "**/__pycache__": true,
        "**/.mypy_cache": true
    },
    "[javascript]": {
        "editor.defaultFormatter": "Glavin001.unibeautify-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "Glavin001.unibeautify-vscode"
    }
}